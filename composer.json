{
    "name": "devpress/address-toolkit",
    "description": "",
    "keywords": [ "wordpress" ],
    "type": "wordpress-plugin",
    "homepage": "https://github.com/devpress/address-toolkit-for-woocommerce",
    "license": "GPL-2.0+",
    "authors": [
        {
            "name": "Devin Price",
            "homepage": "https://devpress.com"
        }
    ],
    "support": {
